# Criar o DataFrame
df = pd.DataFrame(data)

# Converter a coluna de data para o formato correto
df['Date'] = pd.to_datetime(df['Date'])

# Plotar diferentes tipos de gr치ficos para an치lise

# Gr치fico de linhas para as vari치veis 'Open' e 'Close' ao longo do tempo
plt.figure(figsize=(10,6))
plt.plot(df['Date'], df['Open'], label='Open', marker='o')
plt.plot(df['Date'], df['Close'], label='Close', marker='x')
plt.xlabel('Date')
plt.ylabel('Price')
plt.title('Open vs Close Price Over Time')
plt.xticks(rotation=45)
plt.legend()
plt.tight_layout()
plt.show()
